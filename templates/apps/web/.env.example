# API Configuration
VITE_API_BASE_URL=https://your-api-domain.cloudfront.net

# Cognito Configuration (when using Cognito auth provider)
VITE_COGNITO_USER_POOL_ID=
VITE_COGNITO_CLIENT_ID=
