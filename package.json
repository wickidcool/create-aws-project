{
  "name": "create-aws-project",
  "version": "1.5.0",
  "description": "CLI tool to scaffold AWS projects",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "create-aws-project": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:e2e": "node --import tsx/esm src/__tests__/harness/local-runner.ts",
    "test:e2e:smoke": "node --import tsx/esm src/__tests__/harness/local-runner.ts smoke",
    "test:e2e:full": "node --import tsx/esm src/__tests__/harness/local-runner.ts full",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "files": [
    "dist",
    "templates",
    "README.md"
  ],
  "keywords": [
    "aws",
    "starter-kit",
    "cli",
    "scaffold",
    "nx",
    "monorepo"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/wickidcool/create-aws-project.git"
  },
  "author": "wickidcool",
  "homepage": "https://github.com/wickidcool/create-aws-project#readme",
  "bugs": {
    "url": "https://github.com/wickidcool/create-aws-project/issues"
  },
  "dependencies": {
    "@aws-sdk/client-iam": "^3.971.0",
    "@aws-sdk/client-organizations": "^3.700.0",
    "@aws-sdk/client-sts": "^3.972.0",
    "@aws-sdk/credential-providers": "^3.971.0",
    "@octokit/rest": "^22.0.1",
    "find-up": "^8.0.0",
    "ora": "^9.1.0",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2",
    "libsodium-wrappers": "^0.7.15",
    "validate-npm-package-name": "^7.0.2"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/libsodium-wrappers": "^0.7.14",
    "@types/node": "^24.10.1",
    "@types/prompts": "^2.4.9",
    "@types/validate-npm-package-name": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "execa": "^9.6.1",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
